<div id="answer-10162700" class="answer" data-answerid="10162700"  itemscope itemtype="http://schema.org/Answer">
    <table>
        <tr>
            <td class="votecell">
                

<div class="vote">
        <input type="hidden" name="_id_" value="10162700">
        <a class="vote-up-off" title="This answer is useful">up vote</a>
        <span itemprop="upvoteCount" class="vote-count-post ">234</span>
        <a class="vote-down-off" title="This answer is not useful">down vote</a>




